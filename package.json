{
  "name": "docgen-tool",
  "version": "3.0.0",
  "description": "A tool for creating HTML and PDF documentation",
  "main": "docgen",
  "bin": {
    "docgen": "./docgen"
  },
  "scripts": {
    "test": "npm run prettier:check",
    "prettier:check": "prettier --check 'docgen' '*.{js,json,css,less}'",
    "prettier:fix": "prettier --write 'docgen' '*.{js,json,css,less}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,less}": "prettier --write",
    "docgen": "prettier --write"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cheerio": "^1.0.0-rc.12",
    "cli-spinner": "^0.2.10",
    "commander": "^6.2.0",
    "eslint-config-prettier": "^6.15.0",
    "fs-extra": "^11.1.1",
    "husky": "^4.3.0",
    "image-size": "^1.0.2",
    "lint-staged": "^10.5.1",
    "markdown-it": "^13.0.1",
    "moment": "^2.29.4",
    "rsvp": "^4.8.5",
    "spawn-args": "^0.2.0",
    "z-schema": "^6.0.1"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtmacdonald/docgen"
  },
  "author": "Inkit Inc. and contributors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtmacdonald/docgen/issues"
  },
  "homepage": "http://mtmacdonald.github.io/docgen/docs/index.html"
}
